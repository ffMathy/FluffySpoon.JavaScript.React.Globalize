{
  "compilerOptions": {
    "target": "es5",
    "declaration": true,
    "lib": [],
    "outDir": "./dist",
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "isolatedModules": true
  },
  "compileOnSave": true,
  "include": [
    "src"
  ]
}
